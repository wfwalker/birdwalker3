<% county = onelinelink %>

<div class="pagecontentitem">
	<% link_title = "County report, " + county.name + " County, " + county.state.abbreviation %>
	<%= link_to county.name + " County", county_url(county), :title => link_title %><% if (show_extras) %>, <%= county.state.abbreviation %><% end %>
</div>
