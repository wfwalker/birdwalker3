<div class="lead">
	<%= @page_title %>
	<% editing_is_allowed?() do %>
		<span style="font-size: 9pt"><%= link_to 'new', new_county_url %></span>
	<% end %>
</div>

<% county_lists = @counties/(@counties.size / 4 + 4) %>

<div class='row'>
	<% for county_list in county_lists do %>
		<div class='span3'>
			<%= render :partial => 'counties/divided_list', :object => county_list, :locals => { :show_headings => false, :show_extras => false } %>
		</div>
	<% end %>
</div>
