<div class="lead">
	<%= @page_title %> in <%= link_to @county.state.name, state_url(@county.state) %>
		<% editing_is_allowed?() do %>
		<span style="font-size: 9pt"><%= link_to 'edit', edit_county_url(@county) %></span>
	<% end %>       
</div> 

<% if @county.photos.length > 3 %>
	<%= render :partial => 'photos/gallery', :object => @county.photos %>
<% end %>

<div class="pagecontentmodule">
	<div class="moduletitle"><%= @county.species.length %> Species
	</div>
</div>

<%= render :partial => 'sightings/species_by_month_table', :object => @map, :locals => { :totals => @totals }%>

