<% state = page_header%>

<% if (! iphone?) %>
	<div id="pageheader-location">
		<div id="pagetitle"><%= @page_title %></div>
		<div class="pageheaderitem">
			<%= link_to 'normal view', :id => state, :action => 'show' %> | 
			<%= link_to 'by year view', :id => state, :action => 'show_species_by_year' %> |
			<%= link_to 'by month view', :id => state, :action => 'show_species_by_month' %>
		</div>
	</div>
<% end %>