<div id="pagebody">
	<div id="pageheader">
		<div id="pagetitle">
		</div>
	</div>

	<div id="pagecontent">

		<div id='column1of2'>
			
			<%= render :partial => 'bird_walker/welcome' %>   
			
			<% if @bird_of_the_week != nil %>
				<%= render :partial => 'bird_walker/see_it_this_week' %>   
            <% end %>

			<%= render :partial => 'bird_walker/this_year_so_far' %>
								
		</div>
		<div id='column2of2'>

			<div class="pagecontentmodule">
				<div class="moduletitle">Recent Trip Reports</div>
			</div>                                         
			
			<%= render :partial => 'trips/summary', :collection => @recent_trips %>    
			
   		</div>
	</div>
</div>