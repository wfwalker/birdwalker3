<div id="pagebody">
	<div id="pageheader">
		<div id="pagetitle">Search Results</div>
	</div>
	<div id="pagecontent">
		<div class="pagecontentmodule">
		<div class="moduletitle">Search Results for '<%= @terms %>'</div>
			<% if (@found_species != nil && @found_species.size > 0) then %>
				<%= show_species_list(@found_species, true) %>
			<% end %>

			<% if (@found_trips != nil && @found_trips.size > 0) then %>
				<%= show_trip_list(@found_trips, true) %>
			<% end %>
			
			<% if (@found_locations != nil && @found_locations.size > 0) then %>
				<%= show_location_list(@found_locations, true) %>
			<% end %>
			
			<% if (@found_counties != nil && @found_counties.size > 0) then %>
				<%= show_county_list(@found_counties, true) %>
			<% end %>
		</div>
	</div>
</div>