<h3><%= @page_title %></h3>

<div class="pagecontentmodule">  
	<h4>Location Galleries</h4>
	<% for location in @location_galleries %>
		<div class="pagecontentitem">
		<%= link_to location.name, :action => 'show', :controller => 'locations', :id => location %>,
		<%= location.county.state.name%>
		</div>
	<% end %>
</div>               

<div class="pagecontentmodule">  
	<h4>Other</h4>
	<div class="pagecontentitem"><%= link_to "Search", :action => 'photo_search', :controller => 'bird_walker' %></div>
	<div class="pagecontentitem"><%= link_to "Recent", :action => 'recent_gallery', :controller => 'photos' %></div>
</div>

